{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "CMake Create",
            "type": "shell",
            "command": "cmake",
            "args": [
                ".."
            ],
            "options": {
                "cwd": "${workspaceFolder}/build/"
            },
        },
        {
            "label": "CMake Build",
            "type": "shell",
            "command": "cmake",
            "args": [
                "--build",
                "."
            ],
            "options": {
                "cwd": "${workspaceFolder}/build/"
            },
        },
        {
            "label": "CMake Clean",
            "type": "shell",
            "command": "rm",
            "args": [
                "-rf",
                "*"
            ],
            "options": {
                "cwd": "${workspaceFolder}/build/"
            },
        },
        {
            "label": "Run Main Group",
            "type": "shell",
            "command": "cmake",
            "dependsOrder": "sequence",
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "dependsOn": [
                "CMake Clean",
                "CMake Create",
                "CMake Build"
            ]
        },


        


        {
            "label": "CMake main_log",
            "type": "shell",
            "command": "cmake",
            "args": [
                "..",
                "-Dmain_log=1"
            ],
            "options": {
                "cwd": "${workspaceFolder}/build/"
            },
        },
        {
            "label": "Run main_log",
            "type": "shell",
            "command": "cmake",
            "dependsOrder": "sequence",
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "dependsOn": [
                "CMake main_log",
                "CMake Build"
            ]
        }



    ]
}